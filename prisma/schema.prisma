datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Todo {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  title    String
  body     String?
  completed Boolean @default(false)
  user_id String
  createdAt DateTime @default(now())
}




